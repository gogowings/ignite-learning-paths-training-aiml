    <command:details>
      <command:name>Backup-Location</command:name>
      <command:verb>Backup</command:verb>
      <command:noun>Location</command:noun>
      <maml:description>
        <maml:para>Sets the location n number of levels up.</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>You no longer have to cd ..\..\..\..\ to move back four levels. You can now just type bu 4</maml:para>
      <maml:para>Aliases: bu</maml:para>
    </maml:description>
